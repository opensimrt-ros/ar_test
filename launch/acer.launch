<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <node name="usb_cam_acer" pkg="usb_cam" type="usb_cam_node" output="log" >
    <param name="video_device" value="/dev/video4" />
    <!-- this camera, acer acr010 has a nice resolution but terribly low refresh rate, so we will set the max -->
    <!-- param name="image_width" value="1920" />
    <param name="image_height" value="1080" />
    <param name="pixel_format" value="yuyv" />
    <param name="framerate" value="30"/ measured rate was 5hz-->
    <!-- param name="image_width" value="1280" />
    <param name="image_height" value="720" />
    <param name="pixel_format" value="yuyv" />
    <param name="framerate" value="30"/ measure rate was 10-->
    <param name="image_width" value="640" />
    <param name="image_height" value="480" />
    <param name="pixel_format" value="yuyv" />
    <param name="framerate" value="30"/>
    <param name="autoexposure" value="true"/>
    <param name="camera_frame_id" value="usb_cam_acer" />
    <param name="io_method" value="mmap"/>
    <param name="camera_info_url" value="package://ar_test/acer_acr010_640_480.yaml"/>
    <!-- param name="camera_info_url" value="package://ar_test/acer_acr010.yaml"/ 1920x1080 calibration, but the ar doesnt like this-->
  </node>
</launch>
